	<JobDef>
		<defName>RSFuelRobot</defName>
		<driverClass>RimsecSecurity.JobDriver_FuelRobot</driverClass>
		<reportString>Fueling TargetA.</reportString>
		<casualInterruptible>false</casualInterruptible>
	</JobDef>

  <JobDef>
    <defName>RSManualRepair</defName>
    <driverClass>RimsecSecurity.JobDriver_ManualRepair</driverClass>
    <reportString>Repairing Robot at TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>

